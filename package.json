{
  "name": "fastify-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "src/web/server.js",
  "type": "module",
  "scripts": {
    "start": "node .",
    "format": "yarn prettier --write *.js",
    "lint": "eslint .",
    "test": "nyc mocha --r esm --timeout 5000 tests --recursive --exit && yarn lint && yarn run vulnerabilities",
    "vulnerabilities": "npx audit-ci -h"
  },
  "engines": {
    "node": "^16.0.1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^10.0.0",
    "fastify": "^3.22.1",
    "fastify-auth": "^1.0.1",
    "fastify-auth0-verify": "^0.6.0",
    "fastify-basic-auth": "^2.1.0",
    "fastify-jwt-authz": "^0.1.2",
    "lodash": "^4.17.21",
    "throng": "^5.0.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.20.1",
    "mocha": "^9.1.3",
    "nyc": "^15.0.0",
    "prettier": "^2.4.1"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 80,
    "statements": 80,
    "functions": 80,
    "branches": 80,
    "exclude": [
      "tests/",
      "src/web/server.js",
      "src/web/app.js",
      "src/routes",
      "src/plugins"
    ]
  }
}
